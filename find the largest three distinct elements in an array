import sys

def print3largest(arr, arr_size):
    
    if (arr_size < 3):
        
        print("Invalid Input")
        return
    
    third = first = second = -sys.maxsize
    
    for i in range(0, arr_size):
        
        if (arr[i] > first):
            
            third = second
            second = first
            first = arr[i]
            
        elif (arr[i] > second):
            
            third = second
            second = arr[i]
            
        elif (arr[i] > third):
            third = arr[i]
            
    print("Three largest elements are",first,second,third)
    
arr = list(map(int,input().split()))
n = len(arr)
print3largest(arr,n)
